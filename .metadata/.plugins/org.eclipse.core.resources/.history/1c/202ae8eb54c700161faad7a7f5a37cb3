enum Outcome {
	WIN, LOSE, DRAW
}

interface Item {
	Outcome compete(Item it);

	Outcome eval(Paper p);

	Outcome eval(Scissors p);

	Outcome eval(Rock p);
}

class Paper implements Item {

	@Override
	public Outcome compete(Item it) {
		return it.eval( this );
	}

	@Override
	public Outcome eval(Paper p) {
