package share.evenchecker;

public class EvenGenerator extends IntGenerator {

	private int value = 0;

	@Override
	public int next() {
		++value;
		++value;
		return value;
	}

	public static void main(String[] args) {

		// 测试EvenGenerator
		IntGenerator g = new EvenGenerator();
		EvenChecker.test( g );
	}
}
