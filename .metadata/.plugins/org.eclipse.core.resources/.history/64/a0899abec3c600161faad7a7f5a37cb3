package basic;

public enum EnumWithMethod {
	// enum实例必须在前面
	WEST("This is WEST"), 
	NORTH("This is North"), 
	EAST("This is EAST"), 
	SOUTH("This is South");

	private String description;

	public String getDescription() {
		return description;
	}

	EnumWithMethod(String description) {
		this.description = description;
	}
}
