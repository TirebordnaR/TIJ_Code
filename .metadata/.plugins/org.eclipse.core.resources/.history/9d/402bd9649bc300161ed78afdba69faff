package share.evenchecker;

public class EvenGenerator extends IntGenerator {

	private int value = 0;

	@Override
	public int next() {
//		++value;
//		++value;
		value = 2;
		return value;
	}

	public static void main(String[] args) {

		IntGenerator g = new EvenGenerator();
		EvenChecker.test( g );
	}
}
